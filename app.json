    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vutran2203.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "4a6c09b9-8931-4116-ace6-d8cf582a8ed3"
      }
    }
  }
